    "adm-zip": "^0.5.9",
    "aws-sdk": "^2.1192.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
